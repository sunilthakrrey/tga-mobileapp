    <!--Common Styles Start-->

    <Style x:Key="CommonCustomEntryStyle" TargetType="customControls:CustomEntry">
        <Setter Property="FontSize" Value="{StaticResource lblNormal}"/>
        <Setter Property="FontFamily" Value="Montserrat-SemiBold"/>
        <Setter Property="PlaceholderColor" Value="#fff"/>
    </Style>

    <Style x:Key="CommonBtnOrangeStyle" TargetType="Button">
        <Setter Property="FontSize" Value="{StaticResource lblXtraLarge}"/>
        <Setter Property="TextColor" Value="#fff"/>
        <Setter Property="FontFamily" Value="Montserrat-Bold"/>
        <Setter Property="CornerRadius" Value="50"/>
        <Setter Property="TextTransform" Value="None"/>
        <Setter Property="VerticalOptions" Value="Center"/>
        <Setter Property="HorizontalOptions" Value="Center"/>
        <Setter Property="BackgroundColor" Value="#FCC443"/>
    </Style>

    <Style x:Key="CommonBtnOrangeContainerStyle" TargetType="panckeview:PancakeView">
        <Setter Property="CornerRadius" Value="50"/>
        <Setter Property="HeightRequest" Value="45"/>
        <Setter Property="WidthRequest" Value="159"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="HorizontalOptions" Value="Center"/>
        <Setter Property="VerticalOptions" Value="Start"/>
    </Style>

    <!--Common Styles Start-->




    <!--#region Login Page-->

    <Style x:Key="LoginPageMainContainerStyle" TargetType="Grid">
        <Setter Property="Padding" Value="0"/>
        <Setter Property="Margin" Value="0"/>
        <Setter Property="VerticalOptions" Value="FillAndExpand"/>
        <Setter Property="HorizontalOptions" Value="FillAndExpand"/>
    </Style>

    <Style x:Key="LoginPageSubContainerStyle" TargetType="Grid">
        <Setter Property="Padding" Value="30"/>
        <Setter Property="VerticalOptions" Value="FillAndExpand"/>
    </Style>

    <Style x:Key="lblWelcomeBackStyle" TargetType="Label">
        <Setter Property="FontSize" Value="{StaticResource lblExceptionLarge}"/>
        <Setter Property="TextColor" Value="#fff"/>
        <Setter Property="VerticalOptions" Value="EndAndExpand"/>
        <Setter Property="HorizontalOptions" Value="Center"/>
        <Setter Property="FontFamily" Value="Pacifico-Regular"/>
    </Style>

    <Style x:Key="EmailEntryMainContainerStyle" TargetType="StackLayout">
        <Setter Property="Spacing" Value="16"/>
        <Setter Property="Margin" Value="0,25,0,0"/>
        <Setter Property="VerticalOptions" Value="End"/>
    </Style>

    <Style x:Key="EmailEntryContainerStyle" TargetType="Frame">
        <Setter Property="BackgroundColor" Value="#B1DD96"/>
        <Setter Property="HasShadow" Value="False"/>
        <Setter Property="CornerRadius" Value="50"/>
        <Setter Property="Margin" Value="0"/>
        <Setter Property="Padding" Value="15,1"/>
    </Style>

    <Style x:Key="lblForClickHereContainerStyle" TargetType="StackLayout">
        <Setter Property="Margin" Value="40,0"/>
    </Style>

    <Style x:Key="lblForClickHereStyle" TargetType="Label">
        <Setter Property="FontSize" Value="{StaticResource lblSmall}"/>
        <Setter Property="TextColor" Value="#fff"/>
        <Setter Property="FontFamily" Value="Montserrat-Regular"/>
    </Style>

    <Style x:Key="lblWarningContainerStyle" TargetType="StackLayout">
        <Setter Property="Orientation" Value="Horizontal"/>
    </Style>

    <Style x:Key="ImageWarningIconStyle" TargetType="ffsvg:SvgCachedImage">
        <Setter Property="HeightRequest" Value="20"/>
        <Setter Property="WidthRequest" Value="20"/>
        <Setter Property="Aspect" Value="AspectFit"/>
        <Setter Property="VerticalOptions" Value="Start"/>
        <Setter Property="Margin" Value="0,5"/>
    </Style>

    <Style x:Key="lblWarningStyle" TargetType="Label" BasedOn="{StaticResource lblForClickHereStyle}">
        <Setter Property="LineHeight" Value="1.2"/>
        <Setter Property="Padding" Value="15,0,0,0"/>
    </Style>

    <Style x:Key="EmailEntryStyle" TargetType="customControls:CustomEntry" BasedOn="{StaticResource CommonCustomEntryStyle}">

    </Style>

    <Style x:Key="lblForgetPasswordStyle" TargetType="Label">
        <Setter Property="FontSize" Value="{StaticResource lblSmall}"/>
        <Setter Property="TextColor" Value="#fff"/>
        <Setter Property="FontFamily" Value="Montserrat-SemiBold"/>
        <Setter Property="HorizontalOptions" Value="End"/>
    </Style>

    <Style x:Key="lblRememberMeContainerStyle" TargetType="StackLayout">
        <Setter Property="Padding" Value="0"/>
        <Setter Property="Spacing" Value="0"/>
        <Setter Property="Orientation" Value="Horizontal"/>
        <Setter Property="VerticalOptions" Value="Center"/>
    </Style>

    <Style x:Key="lblRememberMeStyle" TargetType="Label">
        <Setter Property="FontSize" Value="{StaticResource lblMedium}"/>
        <Setter Property="TextColor" Value="#fff"/>
        <Setter Property="FontFamily" Value="Montserrat-SemiBold"/>
        <Setter Property="VerticalOptions" Value="Center"/>
    </Style>

    <Style x:Key="CheckBoxStyle" TargetType="CheckBox">
