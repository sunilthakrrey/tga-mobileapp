    </Style>

    <Style x:Key="BtnLoginStyle" TargetType="Button" BasedOn="{StaticResource CommonBtnOrangeStyle}">
        <Setter Property="HeightRequest" Value="45"/>
        <Setter Property="WidthRequest" Value="159"/>
    </Style>

    <Style x:Key="BtnLoginContainerStyle" TargetType="panckeview:PancakeView" BasedOn="{StaticResource CommonBtnOrangeContainerStyle}">
        <Setter Property="Margin" Value="0,15,0,0"/>
    </Style>

    <Style x:Key="ImageSocialIconContainerStyle" TargetType="StackLayout">
        <Setter Property="Spacing" Value="18"/>
        <Setter Property="Margin" Value="0,0,0,10"/>
        <Setter Property="Orientation" Value="Horizontal"/>
        <Setter Property="VerticalOptions" Value="End"/>
        <Setter Property="HorizontalOptions" Value="Center"/>
    </Style>

    <Style x:Key="ImageSocialIconStyle" TargetType="ffsvg:SvgCachedImage">
        <Setter Property="HeightRequest" Value="30"/>
        <Setter Property="WidthRequest" Value="30"/>
    </Style>






    <Style x:Key="lblLoginTagLine" TargetType="Label" BasedOn="{StaticResource CommonStyleTestingBase}">
        <Setter Property="FontSize" Value="20"></Setter>
        <Setter Property="TextColor" Value="blue"></Setter>
    </Style>
