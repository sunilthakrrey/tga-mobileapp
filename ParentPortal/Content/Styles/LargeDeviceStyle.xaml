    <Style x:Key="RadioButtonChangeStyle" TargetType="RadioButton">
        <Setter Property="VisualStateManager.VisualStateGroups">
            <VisualStateGroupList>
                <VisualStateGroup x:Name="CheckedStates">
                    <VisualState x:Name="Checked">
                        <VisualState.Setters>
                            <Setter Property="TextColor" Value="{DynamicResource Gray}" />
                            <Setter Property="Opacity" Value="1" />
                            <Setter Property="FontFamily" Value="Montserrat-Bold"/>
                        </VisualState.Setters>
                    </VisualState>
                    <VisualState x:Name="Unchecked">
                        <VisualState.Setters>
                            <Setter Property="TextColor" Value="{DynamicResource SilverChalice}" />
                            <Setter Property="Opacity" Value="0.5" />
                            <Setter Property="FontFamily" Value="Montserrat-SemiBold"/>
                        </VisualState.Setters>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateGroupList>
        </Setter>
        <Setter Property="FontSize" Value="{StaticResource lblMedium}"/>
    </Style>

