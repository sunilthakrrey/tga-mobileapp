    <!--ForgotPasswordPage Style Begin-->

    <Style x:Key="ForgotPasswordMainContainerStyle" TargetType="Grid" BasedOn="{StaticResource LoginPageMainContainerStyle}">

    </Style>

    <Style x:Key="ForgotPasswordSubContainerStyle" TargetType="Grid" BasedOn="{StaticResource LoginPageSubContainerStyle}">

    </Style>

    <Style x:Key="lblResetPasswordStyle" TargetType="Label" BasedOn="{StaticResource lblWelcomeBackStyle}">

    </Style>

    <Style x:Key="EntryEmailEnterMainContainerStyle" TargetType="StackLayout">
        <Setter Property="VerticalOptions" Value="Center"></Setter>
    </Style>

    <Style x:Key="EntryEmailEnterContainerStyle" TargetType="Frame" BasedOn="{StaticResource EmailEntryContainerStyle}">

    </Style>

    <Style x:Key="EntryEmailEnterStyle" TargetType="customControls:CustomEntry" BasedOn="{StaticResource CommonCustomEntryStyle}">

    </Style>

    <Style x:Key="BtnResetPasswordStyle" TargetType="Button" BasedOn="{StaticResource CommonBtnOrangeStyle}">
        <Setter Property="Padding" Value="35,5"/>
    </Style>

    <Style x:Key="BtnResetPasswordContainerStyle" TargetType="panckeview:PancakeView">
        <Setter Property="VerticalOptions" Value="Start"/>
        <Setter Property="CornerRadius" Value="50"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="HorizontalOptions" Value="Center"/>
    </Style>

    <Style x:Key="lblDidntReceivedMailStyle" TargetType="Label">
        <Setter Property="FontSize" Value="{StaticResource lblNormal}"/>
        <Setter Property="TextColor" Value="#fff"/>
        <Setter Property="FontFamily" Value="Montserrat-SemiBold"/>
        <Setter Property="HorizontalOptions" Value="Center"/>
